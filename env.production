# =============================================================================
# OPERABASE BACKEND - PRODUCTION ENVIRONMENT
# AWS Elastic Beanstalk - SÃ£o Paulo (sa-east-1)
# =============================================================================

# Environment
NODE_ENV=production
PORT=8080

# AWS Configuration
AWS_REGION=sa-east-1

# Database - Supabase Production
SUPABASE_URL=your_production_supabase_url
SUPABASE_ANON_KEY=your_production_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_production_supabase_service_role_key

# Redis - AWS ElastiCache
REDIS_URL=redis://your-production-redis-cluster.cache.amazonaws.com:6379

# AI Services
OPENAI_API_KEY=your_production_openai_api_key
ANTHROPIC_API_KEY=your_production_anthropic_api_key

# WhatsApp Integration
EVOLUTION_API_URL=your_production_evolution_api_url
EVOLUTION_API_KEY=your_production_evolution_api_key

# Google Services
GOOGLE_CLIENT_ID=your_production_google_client_id
GOOGLE_CLIENT_SECRET=your_production_google_client_secret
GOOGLE_REDIRECT_URI=your_production_google_redirect_uri

# N8N Integration
N8N_WEBHOOK_URL=your_production_n8n_webhook_url
N8N_API_KEY=your_production_n8n_api_key

# Security
JWT_SECRET=your_super_secure_jwt_secret_for_production
ENCRYPTION_KEY=your_32_character_encryption_key_here

# File Upload
MAX_FILE_SIZE=10485760
ALLOWED_FILE_TYPES=pdf,doc,docx,txt,jpg,jpeg,png

# API Configuration
API_RATE_LIMIT=1000
API_TIMEOUT=30000

# Monitoring
LOG_LEVEL=info
ENABLE_METRICS=true

# CORS
CORS_ORIGIN=https://your-frontend-domain.vercel.app

# =============================================================================
# IMPORTANT: Replace all placeholder values with your actual production values
